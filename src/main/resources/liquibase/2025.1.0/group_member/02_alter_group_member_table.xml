<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.24.xsd">
<changeSet id="1" author="nit208">

    <preConditions onFail="MARK_RAN">
        <not>
            <columnExists tableName="group_member" columnName="ref_group"/>
        </not>
        <not>
            <columnExists tableName="group_member" columnName="ref_user"/>
        </not>
    </preConditions>
    <sql>
        ALTER TABLE group_member
        ALTER COLUMN ref_group SET NOT NULL;
    </sql>

    <sql>
        ALTER TABLE group_member
        ALTER COLUMN ref_user SET NOT NULL;
    </sql>

</changeSet>
</databaseChangeLog>